args = "PhotoVault Server"

[[workflows.workflow]]
name = "PhotoVault Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "--workers", "2", "--timeout", "120", "--graceful-timeout", "30", "--threads", "2", "wsgi:app"]
build = ["pip", "install", "-r", "requirements.txt"]
